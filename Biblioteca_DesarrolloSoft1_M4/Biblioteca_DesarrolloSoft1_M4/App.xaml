<Application x:Class="Biblioteca_DesarrolloSoft1_M4.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Biblioteca_DesarrolloSoft1_M4"
             StartupUri="Vistas/Login.xaml">
    <Application.Resources>
        <Style x:Key="password" >
            <Setter Property="TextElement.FontFamily" Value="/Font/#password"/>
        </Style>
        <Style x:Key="poppins">
            <Setter Property="TextElement.FontFamily" Value="/Font/#Poppins"/>
        </Style>
    </Application.Resources>
</Application>
