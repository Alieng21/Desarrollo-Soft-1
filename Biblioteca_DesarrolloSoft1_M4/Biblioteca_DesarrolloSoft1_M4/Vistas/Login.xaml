<Window x:Class="Biblioteca_DesarrolloSoft1_M4.Vistas.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Biblioteca_DesarrolloSoft1_M4.Vistas"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnlogin" Content="Login" HorizontalAlignment="Left" Margin="152,253,0,0" VerticalAlignment="Top" Width="80" Click="btnlogin_Click"/>
        <TextBox x:Name="txtusuario" HorizontalAlignment="Left" Margin="132,134,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="txtpassword" HorizontalAlignment="Left" Margin="132,181,0,0" PasswordChar="*" VerticalAlignment="Top" Width="120" />

    </Grid>
</Window>
